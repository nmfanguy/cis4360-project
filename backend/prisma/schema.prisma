datasource db {
  provider = "sqlite"
  url      = "file:../data.db"
}

generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = 5
}

model User {
  id       Int     @id @default(autoincrement())
  username String
  password Bytes
  logins   Login[]
}

model Login {
  id       Int    @id @default(autoincrement())
  owner    User   @relation(fields: [userId], references: [id])
  userId   Int
  username Bytes
  password Bytes
  service  String
}
